<div class="container" id="resultEvent">
		<h4>Search Results</h4>
		
		{{#each this}}
		{{#unless joined}}
		{{#unless created}}
			<div class="event-container">
				<a href="/resultEventDetail/{{name}}/{{startTime}}/{{endTime}}/{{category}}/{{description}}">
					<div class="event">
						<h4>{{name}}</h4>
						<h5>{{startTime}} to {{endTime}}</h5>
						<p>{{description}}</p>
					</div>
				</a>
			</div>
		{{/unless}}
		{{/unless}}
	{{/each}}
	
	<a href="/findEvents"><button class="btn btn-primary col-xs-12 btn-lg">Back</button></a>
</div>