<body>
    <div class="navbar-fixed">
        <nav>
            <div class="nav-wrapper">
                <a href="#" data-activates="mobile-demo" class="button-collapse right"><i class="material-icons">menu</i></a>
                <ul class="right hide-on-med-and-down">
                    <li><a href="/myProfile">My Profile</a></li>
                    <li><a href="/messages">Messages</a></li>
                    <li><a href="/contactUs">Contact Us</a></li>
                    <li><a href="/logout">Logout</a></li>
                </ul>
                <ul class="side-nav center-align" id="mobile-demo">
                    <li><a href="/myProfile">My Profile</a></li>
                    <li><a href="/messages">Messages</a></li>
                    <li><a href="/contactUs">Contact Us</a></li>
                    <li><a href="/logout">Logout</a></li>
                </ul>
            </div>
        </nav>
    </div>
    <!-- NAVIGATION BAR END-->

    <!-- CONTENT -->
    <main>
			<div class="container index">
				{{#if event.variant}}
					<ul id="index_tabs" class="tabs">
						<li class="tab col s3"><a class="active" href="#created_tab">Created Events</a></li>
						<li class="tab col s3"><a href="#joined_tab">Joined Events</a></li>
					</ul>
					<div id="created_tab" class="col s12">
						<a id="createB_btn" class="ab_test btn-floating btn-large waves-effect waves-light teal" href="/createEvents"><i class="material-icons">add</i></a>
						{{#each event}}
							{{#if created}}
								{{#if notCancelled}}
									<div class="event-container card-panel">
										<a href="/createdEventDetail/{{eventId}}">
											<div class="event">
												<h5>{{name}}</h5>
												<h5>{{startTime}} to {{endTime}}</h5>
												<p>{{description}}</p>
											</div>
										</a>
									</div>
								{{/if}}
							{{/if}}
						{{/each}}
					</div>
					<div id="joined_tab" class="col s12">
						<a class="btn-floating btn-large waves-effect waves-light teal" href="/findEvents"><i class="material-icons">search</i></a>
						{{#each event}}
							{{#if joined}}
								<div class="event-container card-panel">
									<a href="/joinedEventDetail/{{eventId}}">
										<div class="event">
											<h4>{{name}}</h4>
											<h5>{{startTime}} to {{endTime}}</h5>
											<p>{{description}}</p>
										</div>
									</a>
								</div>
							{{/if}}
						{{/each}}
					</div>

				{{else}}
					<ul class="nav nav-tabs">
								<li id="createdA_tab" class="active"><a href="#">Created Events</a></li>
								<li id="joinedA_tab"><a href="/joinedEvents">Joined Events</a></li>
						</ul>				
					{{#each event}}
						{{#if created}}
							{{#if notCancelled}}
								<div class="event-container card-panel">
									<a href="/createdEventDetail/{{eventId}}">
										<div class="event">
											<h5>{{name}}</h5>
											<h5>{{startTime}} to {{endTime}}</h5>
											<p>{{description}}</p>
										</div>
									</a>
								</div>
							{{/if}}
						{{/if}}
					{{/each}}
					<a href="/createEvents"><button id="createdA_btn" class="ab_test btn btn-primary col-xs-12 btn-lg">Create An Event</button></a>
				{{/if}}

			</div>

			</main>
    <!-- CONTENT END -->

    <!-- BOTTOM NAVIGATION BAR -->
    <footer class="page-footer">
        <div class="container">
            <div class="row">
                <div class="col s4 right-align">
									<a class="white-text" href="/createEvents">
<!--										<i class="fa fa-plus fa-2x"></i>-->
										<i class="material-icons plus small">add</i>
<!--										<p>Create</p>-->
									</a>
                </div>
                <div class="col s4 center-align">
                    <a class="white-text" href="/index">
<!--											<i class="fa fa-home fa-2x"></i>-->
											<i class="material-icons small">home</i>
<!--											<p>Home</p>-->
									</a>
                </div>
                <div class="col s4 left-align ">
									<a class="white-text" href="/findEvents">
<!--										<i class="fa fa-search fa-2x"></i>-->
										<i class="material-icons small">search</i>
<!--										<p>Find</p>-->
									</a>
                </div>
            </div>
        </div>
    </footer>
	
</body>

</html>
					
