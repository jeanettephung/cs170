	#index_tabs {
		position: fixed;
    top: 57px;
    right: 3px;
    background-color: white;
		z-index: 50;
		
		-webkit-box-shadow: 0px 18px 29px 2px rgba(255,255,255,1);
		-moz-box-shadow: 0px 18px 29px 2px rgba(255,255,255,1);
		box-shadow: 0px 18px 29px 2px rgba(255,255,255,1);
	}
	
	#created_tab, #joined_tab {
		position: relative;
    top: 35px;
	}
	
	.btn-floating {
		position: fixed;
    z-index: 50;
    right: 10px;
    top: 112px;
		
	-webkit-box-shadow: 8px 7px 11px -4px rgba(0,0,0,0.75);
